{
  "expo": {
    "name": "Crush",
    "slug": "Crush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.svg",
    "scheme": "crush",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF1493",
        "foregroundImage": "./assets/images/android-icon-foreground.svg",
        "backgroundImage": "./assets/images/android-icon-background.svg",
        "monochromeImage": "./assets/images/android-icon-monochrome.svg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF1493",
          "dark": {
            "backgroundColor": "#FF1493"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
